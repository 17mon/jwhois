msgstr "versi"

#: src/init.c:124
msgid "Usage: jwhois [OPTIONS] [QUERY]"
msgstr "Penggunaan: jwhois [OPTIONS] [QUERY]"

#: src/init.c:126
msgid ""
"  --version                  display version number and patch level\n"
"  --help                     display this help\n"
"  -v, --verbose              verbose debug output\n"
"  -c FILE, --config=FILE     use FILE as configuration file\n"
"  -h HOST, --host=HOST       explicitly query HOST\n"
"  -n, --no-redirect          disable content redirection\n"
"  -s, --no-whoisservers      disable whois-servers.net service support\n"
"  -a, --raw                  disable reformatting of the query\n"
"  -i, --display-redirections display all redirects instead of hiding them\n"
"  -p PORT, --port=PORT       use port number PORT (in conjunction with "
"HOST)\n"
"  -r, --rwhois               force an rwhois query to be made\n"
"  --rwhois-display=DISPLAY   sets the display option in rwhois queries\n"
"  --rwhois-limit=LIMIT       sets the maximum number of matches to return\n"
msgstr ""
"  --version                  tampilkan nomor versi dan tingkat patch\n"
"  --help                     tampilkan help ini\n"
"  -v, --verbose              output verbose debug\n"
"  -c FILE, --config=FILE     gunakan FILE sebagai file konfigurasi\n"
"  -h HOST, --host=HOST       query HOST secara eksplisit\n"
"  -n, --no-redirect          tiadakan content redirection\n"
"  -s, --no-whoisservers      tiadakan dukungan layanan whois-servers.net\n"
"  -a, --raw                  tiadakan pemformatan ulang query\n"
"  -i, --display-redirections tampilkan semua redirect alih-alih "
"menyembunyikannya\n"
"  -p PORT, --port=PORT       gunakan nomor port PORT (bersama dengan HOST)\n"
"  -r, --rwhois               paksa query rwhois untuk dilakukan\n"
"  --rwhois-display=DISPLAY   set opsi display dalam query rwhois\n"
"  --rwhois-limit=LIMIT       set jumlah maksimum kecocokan yang ditampilkan\n"

#: src/init.c:141
msgid ""
"  -f, --force-lookup         force lookup even if the entry is cached\n"
"  -d, --disable-cache        disable cache functions\n"
msgstr ""
"  -f, --force-lookup         paksa pencarian bahkan bila entri di-cache\n"
"  -d, --disable-cache        tiadakan fungsi cache\n"

