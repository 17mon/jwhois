#: src/init.c:126
msgid ""
"  --version                  display version number and patch level\n"
"  --help                     display this help\n"
"  -v, --verbose              verbose debug output\n"
"  -c FILE, --config=FILE     use FILE as configuration file\n"
"  -h HOST, --host=HOST       explicitly query HOST\n"
"  -n, --no-redirect          disable content redirection\n"
"  -s, --no-whoisservers      disable whois-servers.net service support\n"
"  -a, --raw                  disable reformatting of the query\n"
"  -i, --display-redirections display all redirects instead of hiding them\n"
"  -p PORT, --port=PORT       use port number PORT (in conjunction with HOST)\n"
"  -r, --rwhois               force an rwhois query to be made\n"
"  --rwhois-display=DISPLAY   sets the display option in rwhois queries\n"
"  --rwhois-limit=LIMIT       sets the maximum number of matches to return\n"
msgstr ""
"  --version                  yama seviyesi ve sürüm numarasýný gösterir\n"
"  --help                     bu yardýmý gösterir\n"
"  -v, --verbose              detaylý hata bilgisi verir\n"
"  -c DOSYA, --config=DOSYA   ayar dosyasý olarak DOSYA'yý kullanýr\n"
"  -h MAK, --host=MAK         özellikle MAK makinasýný sorgular\n"
"  -n, --no-redirect          içerik yönlendirmesini etkisizleþtirir\n"
"  -s, --no-whoisservers      whois-servers.net servis desteðini etkisizleþtirir\n"
"  -a, --raw                  sorgunun tekrar biçemlenmesini etkisizleþtirir\n"
"  -i, --display-redirections tekrar yönlendirmeleri gizlemek yerine gösterir\n"
"  -p PORT, --port=PORT       PORT numarasýný kullanýr (MAK ile birlikte)\n"
"  -r, --rwhois               rwhois sorgusuna zorlar\n"
"  --rwhois-display=EKRAN     rwhois sorgularýnda ekran seçeneklerini ayarlar\n"
"  --rwhois-limit=SINIR       verilecek cevap sayýsýný sýnýrlar\n"

#: src/init.c:141
msgid ""
"  -f, --force-lookup         force lookup even if the entry is cached\n"
"  -d, --disable-cache        disable cache functions\n"
msgstr ""
"  -f, --force-lookup         girdi arabellekte olsa bile tekrar sorgular\n"
"  -d, --disable-cache        ara bellek fonksiyonlarýný etkisizleþtirir\n"

