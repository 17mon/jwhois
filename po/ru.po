#: src/init.c:85
msgid "Usage: jwhois [OPTIONS] [QUERY]"
msgstr "Использование: jwhois [ПАРАМЕТРЫ] [ЗАПРОС]"

#: src/init.c:87
msgid ""
"  --version               display version number and patch level\n"
"  --help                  display this help\n"
"  -c FILE, --config=FILE  use FILE as configuration file\n"
"  -h HOST, --host=HOST    explicitly query HOST\n"
"  -p PORT, --port=PORT    use port number PORT (in conjunction with HOST)\n"
"  -v, --verbose           verbose debug output\n"
msgstr ""
"  --version               отобразить номера версии и патча\n"
"  --help                  отобразить эту подсказку\n"
"  -c FILE, --config=FILE  использовать FILE как конфигурационный файл\n"
"  -h HOST, --host=HOST    запрашивать именно этот HOST\n"
"  -p PORT, --port=PORT    использовать номер порта PORT (совместно с HOST)\n"
"  -v, --verbose           Подробный отладочный вывод\n"

#: src/init.c:95
msgid ""
"  -f, --force-lookup      force lookup even if the entry is cached\n"
"  -d, --disable-cache     disable cache functions\n"
msgstr ""
"  -f, --force-lookup      просматривать, даже если запись кэширована\n"
"  -d, --disable-cache     запретить функции кэша\n"

